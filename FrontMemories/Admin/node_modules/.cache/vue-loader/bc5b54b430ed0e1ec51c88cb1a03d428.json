  <div class=\"time\">\n                <span class=\"day text-secondary\"\n                  ><span class=\"textDay\">Samedi </span></span\n                >\n                : De\n                <input\n                  required\n                  v-model=\"satSt\"\n                  class=\"inputtime\"\n                  type=\"time\"\n                  name=\"\"\n                  id=\"\"\n                />\n                A\n                <input\n                  required\n                  v-model=\"satEn\"\n                  class=\"inputtime\"\n                  type=\"time\"\n                  name=\"\"\n                  id=\"\"\n                />\n                \n              </div>\n              \n              <div class=\"time\">\n                <span class=\"day text-secondary\"\n                  ><span class=\"textDay\">Dimanche</span></span\n                >\n              <div class=\"time\">\n                <span class=\"day text-secondary\"\n                  ><span class=\"textDay\">Dimanche</span>\n                </span>\n                : De\n                <input\n                  required\n                  v-model=\"sunSt\"\n                  class=\"inputtime\"\n                  type=\"time\"\n                  name=\"\"\n                  id=\"\"\n                />\n                A\n                <input\n                  required\n                  v-model=\"sunEn\"\n                  class=\"inputtime\"\n                  type=\"time\"\n                  name=\"\"\n                  id=\"\"\n                />\n              </div>\n            </div>\n          </div>\n        </div>\n        <div>\n      \n        </div>\n        \n        <div class=\"text-center\">\n          <button type=\"submit\" class=\"btn-info\">AJOUTER</button>\n        </div>\n        <div class=\"clearfix\"></div>\n      </form>\n    </div>\n  </card>\n</template>\n  <script src=\"../../script/add-tour.js\">\n</script>\n<style scope