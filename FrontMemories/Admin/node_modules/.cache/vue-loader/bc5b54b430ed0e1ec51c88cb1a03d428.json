ed v-model=\"tourCategoryId\">\n              <option\n                :value=\"tourCategory.id\"\n                v-for=\"tourCategory in tourCategoryFromBd\"\n                :key=\"tourCategory.id\"\n              >\n                {{ tourCategory.type }}\n              </option>\n            </select>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-md-4\">\n            <fg-input\n              type=\"file\"\n              label=\"Image\"\n              placeholder=\"image\"\n              accept=\"image/*\"\n              required\n              v-model=\"picture\"\n            >\n            </fg-input>\n          </div>\n          <div class=\"col-md-4\">\n            <fg-input\n              type=\"text\"\n              label=\"Adresse\"\n              placeholder=\"Kara ville\"\n              required\n              v-model=\"address\"\n            >\n            </fg-input>\n          </div>\n          <div class=\"col-md-4\">\n            <label for=\"\">Langues</label>\n            <select class=\"form-control\" multiple required v-model=\"languages\">\n              <option value=\"Français\">Français</option>\n              <option value=\"English\">English</option>\n              <option value=\"Chinese\">Chinese</option>\n              <option value=\"Spanish\">Spanish</option>\n              <option value=\"Deutsh\">Deutsh</option>\n              <option value=\"Haoussa\">Haoussa</option>\n              <option value=\"Ewe\">Ewe</option>\n              <option value=\"Kabye\">Kabye</option>\n              <option value=\"Kotokoli\">Kotokoli</option>\n              <option value=\"Autres\">Autres</option>\n            </select>\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-md-6\">\n            <div class=\"form-group\">\n              <label>Description de site</label>\n              <textarea\n                rows=\"10\"\n                class=\"form-control bord