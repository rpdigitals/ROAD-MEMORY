">\n                  <option\n                    :value=\"restoCategory.id\"\n                    v-for=\"restoCategory in restoCategoryFromBd\"\n                    :key=\"restoCategory.id\"\n                  >\n                    {{ restoCategory.name }}\n                  </option>\n                </select>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-4\">\n                <fg-input\n                  type=\"file\"\n                  label=\"Image\"\n                  placeholder=\"image\"\n                  accept=\"image/*\"\n                  required\n                  v-model=\"picture\"\n                >\n                </fg-input>\n              </div>\n              <div class=\"col-md-4\">\n                <fg-input\n                  type=\"text\"\n                  label=\"Adresse\"\n                  placeholder=\"Kara ville\"\n                  required\n                  v-model=\"address\"\n                >\n                </fg-input>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"form-group\">\n                  <label>Description du restaurant</label>\n                  <textarea\n                    rows=\"10\"\n                    class=\"form-control border-input\"\n                    placeholder=\"Here can be your description\"\n                    maxlength=\"1500\"\n                    required\n                    v-model=\"description\"\n                  >\n                  </textarea>\n                </div>\n              </div>\n              <div class=\"col-md-1\"></div>\n              <div class=\"col-md-5\">\n                <div class=\"form-group ms-50\">\n                  <label>Horaires</label> <br />\n                  <div class=\"time\">\n                    <span class=\"day text-secondary text-secondary\"\n                      ><span class=\"textDay\">Lundi </span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"monSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"monEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Mardi </span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"tueSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"tueEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Mercredi</span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"wedSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n     