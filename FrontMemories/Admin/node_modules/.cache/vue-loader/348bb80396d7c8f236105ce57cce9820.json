      <input\n                      required\n                      v-model=\"monEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Mardi </span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"tueSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"tueEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Mercredi</span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"wedSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"wedEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Jeudi </span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"turSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"turEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Vendredi</span>\n                    </span>\n                    : De\n                    <input\n                      required\n                      v-model=\"friSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"friEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Samedi </span></span\n                    >\n                    : De\n                    <input\n                      required\n                      v-model=\"satSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    <input\n                      required\n                      v-model=\"satEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                  <div class=\"time\">\n                    <span class=\"day text-secondary\"\n                      ><span class=\"textDay\">Dimanche</span>\n                    </span>\n                    : De\n                    <input\n                      required\n                      v-model=\"sunSt\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                    A\n                    <input\n                      required\n                      v-model=\"sunEn\"\n                      class=\"inputtime\"\n                      type=\"time\"\n                      name=\"\"\n                      id=\"\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"text-center\">\n              <button type=\"submit\" class=\"btn-info\" style=\"font-size: 45px\">\n                <i class=\"ti-arrow-right\"></i>\n              </button>\n            </div>\n          </form>\n        </div>\n        <div class=\"clearfix\"></div>\n        <div v-if=\"carForm == 1\">\n          <strong style=\"font-size: 18px\">\n            Caracteristiques et facilités du site\n          </strong>\n          <div class=\"row\">\n            <div class=\"col-md-4\">\n              <label for=\"\">Guide touristique</label>\n              <select class=\"form-control\" v-model=\"tourGuide\">\n                <option value=\"1\">Oui</option>\n                <option value=\"0\">Non</option>\n              </select>\n            </div>\n            <div class=\"col-md-4\">\n              <label for=\"\">Animaux de compagnie autorisés</label>\n              <select class=\"form-control\" v-model=\"animals\">\n                <option value=\"1\">Oui</option>\n                <option value=\"0\">Non</option>\n              </select>\n            </div>\n            <div class=\"col-md-4\">\n              <label for=\"\">Dinner offert</label>\n              <select class=\"form-control\" v-model=\"dinner\">\n                <option value=\"1\">Oui</option>\n                <option value=\"0\">Non</option>\n              </select>\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-4\">\n              <label for=\"\">Cigarettes autorisés</label>\n              <select class=\"form-control\" v-model=\"smokeArea\" required>\n                <option value=\"1\">Oui</option>\n                <option value=\"0\">Non</option>\n              </select>\n            </div>\n            <div class=\"col-md-4\">\n              <label for=\"\">Visite enfants gratuite</label>\n              <select class=\"form-control\" v-model=\"children\">\n                <option value=\"1\">Oui</option>\n                <option value=\"0\">Non</option>\n              </select>\n            </div>\n          </div>\n          <div\n            class=\"alert alert-success text-center\"\n            style=\"font-size: 20px\"\n            v-if=\"tourCreatedSuccessfully == 1\"\n          >\n            <!-- <strong> Site crée avec succès </strong>\n            <strong>\n              <router-link to=\"/services\" class=\"btn btn-success\"\n                >OK</router-link\n              >\n            </strong> -->\n          </div>\n          <div\n            class=\"alert alert-danger text-center\"\n            style=\"font-size: 20px\"\n            v-if=\"tourCreatedSuccessfully == 0\"\n          >\n            <strong>\n              Une erreur s'est produite. Vérifiez votre connexion et réessayez\n            </strong>\n            <strong>\n              <button @click=\"resetTourCreation\" class=\"btn\">OK</button>\n            </strong>\n          </div>\n          <div class=\"text-center\">\n            <button\n              type=\"submit\"\n              @click=\"carForm == 0\"\n              class=\"btn-info\"\n              v-if=\"tourCreatedSuccessfully == 2\"\n            >\n              AJOUTER\n            </button>\n          </div>\n          <div class=\"clearfix\"></div>\n        </div>\n      </form>\n    </div>\n  </card>\n</template>\n<script src=\"../../script/add-tour.js\"></script>\n<style scoped>\n.day {\n  display: block;\n  padding-bottom: 7px;\n  width: 70px;\n}\n.time {\n  display: flex;\n}\n.inputtime {\n  margin-left: 20px;\n  margin-right: 20px;\n  margin-bottom: 8px;\n  border: solid 2px rgb(30, 120, 150);\n  border-radius: 5px;\n}\n.textDay {\n  color: rgb(30, 120, 150);\n}\n.ms {\n  margin-left: 50px;\n}\n\n.btn-info {\n  margin-left: 15px;\n  margin-right: 15px;\n  font-family: Raleway-SemiBold;\n  font-size: 17px;\n  color: rgba(91, 192, 222, 0.75);\n  letter-spacing: 1px;\n  line-height: 22px;\n  border: 2px solid rgba(91, 192, 222, 0.75);\n  border-radius: 30px;\n  background: transparent;\n  transition: all 0.3s ease 0s;\n}\n.btn-info:hover {\n  color: #fff;\n  background: rgba(91, 192, 222, 0.75);\n  border: 2px solid rgba(91, 192, 222, 0.75);\n}\n</style>\n"]}]}